<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:id="@+id/base">

        <shape>
            <stroke android:color="@color/base_black" android:width="@dimen/dash_width"/>
        </shape>
    </item>

    <item
        android:top="@dimen/dash_width"
        android:bottom="@dimen/dash_width"
        >
        <shape android:shape="rectangle">
            <solid android:color="@color/base_item_bg_green" />
        </shape>
    </item>
</layer-list>
